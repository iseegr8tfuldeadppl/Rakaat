<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true"
        android:top="-0dp"
        android:left="-3dp"
        android:right="-3dp"
        android:bottom="0dp">
        <shape>
            <solid android:color="#222222"/>
            <stroke android:color="#1a1a1a" android:width="@dimen/linesstroke"/>
            <corners android:radius="0dp"/>
        </shape>
    </item>
    <item
        android:state_pressed="false"
        android:top="-0dp"
        android:left="-3dp"
        android:right="-3dp"
        android:bottom="0dp">
        <shape>
            <solid android:color="#222222"/>
            <stroke android:color="#1a1a1a" android:width="@dimen/linesstroke"/>
            <corners android:radius="0dp"/>
        </shape>
    </item>
</layer-list>